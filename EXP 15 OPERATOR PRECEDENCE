#include <stdio.h>
void main() 
{
   int a = 20,b = 10,c = 15,d = 5,e;
   e = (a + b) * c / d;      
   printf("Value of (a + b) * c / d is : %d\n",  e );
   e = ((a + b) * c) / d;    
   printf("Value of ((a + b) * c) / d is  : %d\n" ,  e );
   e = (a + b) * (c / d);   
   printf("Value of (a + b) * (c / d) is  : %d\n",  e );
   e = a + (b * c) / d;     
   printf("Value of a + (b * c) / d is  : %d\n" ,  e );
}
